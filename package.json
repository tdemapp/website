{
  "name": "tdem.app",
  "version": "2.1.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:typed": "vuedx-typecheck . && vite build",
    "serve": "vite preview",
    "prettier": "prettier --write \"src/**/*.{js,ts,css,vue}\""
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": true
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.50"
  }
}
